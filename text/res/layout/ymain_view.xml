<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.text.Ymain" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="教学办主界面" />

    <Button
        android:id="@+id/ybutton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ybutton1"
        android:layout_alignBottom="@+id/ybutton1"
        android:layout_marginLeft="25dp"
        android:layout_toRightOf="@+id/ybutton1"
        android:text="选课管理" />

    <Button
        android:id="@+id/ybutton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="17dp"
        android:text="更新计划书" />

    <Button
        android:id="@+id/ybutton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:text="个人信息管理" />

    <Button
        android:id="@+id/ybutton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ybutton3"
        android:layout_below="@+id/ybutton3"
        android:layout_marginTop="51dp"
        android:text="用户管理" />

    <Button
        android:id="@+id/ybutton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ybutton2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:text="注销" />

</RelativeLayout>